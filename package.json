{
  "name": "cos-similarity",
  "version": "1.0.0",
  "description": "Computes the cosine similarity between two vectors",
  "keywords": [
    "array",
    "cos",
    "cosine",
    "difference",
    "distance",
    "math",
    "similarity",
    "vector"
  ],
  "homepage": "https://github.com/rtomrud/cos-similarity#readme",
  "bugs": {
    "url": "https://github.com/rtomrud/cos-similarity/issues"
  },
  "license": "MIT",
  "author": "Artem Rudenko <rtomrud@gmail.com>",
  "files": [
    "dist",
    "index.js"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtomrud/cos-similarity.git"
  },
  "scripts": {
    "bench": "node bench.js",
    "build": "rollup -i index.js -o dist/index.js -f cjs",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "nyc --reporter html tape -r esm index.test.js | faucet && nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "benchmark": "2.1.4",
    "compute-cosine-similarity": "1.0.0",
    "cosine-similarity": "1.0.1",
    "coveralls": "3.0.6",
    "doc-similarity": "0.0.1",
    "eslint": "6.3.0",
    "eslint-config-pretty-strict": "4.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "esm": "3.2.25",
    "faucet": "0.0.1",
    "husky": "3.0.5",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "rollup": "1.20.3",
    "tape": "4.11.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "./node_modules/eslint-config-pretty-strict/index.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "root": true
  },
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "esm": {
    "sourceMap": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
